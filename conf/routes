#Routes for the Course Object
GET  /allcourses  controllers.CourseController.allcourses
GET  /findcoursebyid/:id  controllers.CourseController.findcoursebyid(id: Long)
POST  /createcourse controllers.CourseController.createcourse

#Routes for the Student Object
#GET  /allstudents  controllers.StudentController.allstudents
#GET  /findstudentbydocument/:document  controllers.StudentController.findstudentbydocument(document: Long)
#POST  /createstudent controllers.StudentController.createstudent
